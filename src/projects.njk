---
title: Projects
layout: "base.njk"
pagination:
  data: collections.projects
  size: 9
  reverse: true
---

<h1>{{title}}</h1>
<ul class="articles-list">
  {%- for project in collections.projects -%}

  <li class="project-card">
    <h2>{{ project.data.title }}</h2>
    <p>{{ project.data.description }}</p>
    <img src="{{ project.data.img }}" />
    <a href="{{ project.data.link }}">
      <svg
        xmlns="http://www.w3.org/2000/svg"
        xmlns:xlink="http://www.w3.org/1999/xlink"
        width="29"
        height="28"
        viewBox="0 0 29 28"
        fill="none"
      >
        <rect x="0.84082" width="28" height="28" fill="url(#pattern0)" />
        <defs>
          <pattern
            id="pattern0"
            patternContentUnits="objectBoundingBox"
            width="1"
            height="1"
          >
            <use xlink:href="#image0_255_1193" transform="scale(0.0104167)" />
          </pattern>
          <image
            id="image0_255_1193"
            width="96"
            height="96"
            xlink:href="data:image/png;base64,"
          />
        </defs></svg
    ></a>
  </li>
  {%- endfor -%}
</ul>
